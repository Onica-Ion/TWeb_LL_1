@using System.Web.Optimization
<!DOCTYPE html>

<html>
<head>
     <meta charset="utf-8" />
     <title>Minton - Responsive Admin Dashboard & Landing Page Template</title>
     <meta name="viewport" content="width=device-width, initial-scale=1.0">
     <meta content="A fully featured admin theme which can be used to build CRM, CMS, etc." name="description" />
     <meta content="Coderthemes" name="author" />
     <meta http-equiv="X-UA-Compatible" content="IE=edge" />

     <!-- App favicon -->
     <link rel="shortcut icon" href="~/Content/themes/base/images/favicon.ico">
     @*@Styles.Render("~/Content/themes/base/images/favicon.ico")*@

     <!-- Bootstrap core CSS -->
     @*<link rel="stylesheet" href="~/Content/css/bootstrap.min.css" type="text/css">*@
     @Styles.Render("~/Content/css/bootstrap.min.css")

     <!--Material Icon -->
     @*<link rel="stylesheet" type="text/css" href="~/Content/css/materialdesignicons.min.css" />*@
     @Styles.Render("~/Content/css/materialdesignicons.min.css")

     <!--pe-icon-7 Icon -->
     @*<link rel="stylesheet" type="text/css" href="~/Content/css/pe-icon-7-stroke.css" />*@
     @Styles.Render("~/Content/css/pe-icon-7-stroke.css")

     <!-- Custom  css -->
     @*<link rel="stylesheet" type="text/css" href="~/Content/css/style.css" />*@
     @Styles.Render("~/Content/css/style.css")

</head>
<body>
     <div>
          <nav>
               @Html.Partial("_Header")
          </nav>

          <aside>
               @Html.Partial("_Navigation")
          </aside>

          <section>
               @RenderBody()

               @Html.Partial("_Footer")
          </section>

          <!-- javascript -->
          @Scripts.Render("~/Scripts/js/jquery.min.js")
          @Scripts.Render("~/Scripts/js/bootstrap.bundle.min.js")
          @Scripts.Render("~/Scripts/js/jquery.easing.min.js")
          @Scripts.Render("~/Scripts/js/scrollspy.min.js")

          <!-- counter js -->
          @Scripts.Render("~/Scripts/js/counter.init.js")

          <!-- custom js -->
          @Scripts.Render("~/Scripts/js/app.js")
     </div>
</body>
</html>
